<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="JavaAppDesktopPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>modelo.Administrado</class>
    <class>modelo.Acceso</class>
    <class>modelo.Registro</class>
    <class>modelo.Inscripcion</class>
    <class>modelo.Competicion</class>
    <class>modelo.Usuario</class>
    <class>modelo.Equipo</class>
    <class>modelo.Grupo</class>
    <class>modelo.Compuesta</class>
    <class>modelo.Participante</class>
    <class>modelo.Prueba</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:derby:appDeportivaDB"/>
      <property name="javax.persistence.jdbc.password" value="app"/>
      <property name="javax.persistence.jdbc.driver" value="org.apache.derby.jdbc.EmbeddedDriver"/>
      <property name="javax.persistence.jdbc.user" value="app"/>
    </properties>
  </persistence-unit>
</persistence>
